{
    "sass": {
      "src":  "./scss/*.{sass,scss}",
      "dest": "./css",
    },
    "autoprefixer": {
      "browsers": [
        "last 2 versions",
        "safari 5",
        "ie 8",
        "ie 9",
        "opera 12.1",
        "ios 6",
        "android 4"
      ],
      "cascade": true
    },
    "watch": {
        "sass" : "./scss/*.{sass,scss}"
    }
}
